            <SQLTask:SqlTaskData
              SQLTask:Connection="{31F49E8F-113E-4CDE-B8E9-EEB47A9243CE}"
              SQLTask:SqlStatementSource="-- 1&#xA;UPDATE [dim].[Buyers]&#xA;SET&#xA;&#x9;BuyerGroupCode = tb.BuyerGroupCode,&#xA;&#x9;BuyerGroupName = tb.BuyerGroupName,&#xA;&#x9;[LogisticCode] = tb.LogisticCode,&#xA;&#x9;[BuyerName] = tb.BuyerName,&#xA;&#x9;[BuyerFullName] = tb.BuyerFullName,&#xA;&#x9;Net = tb.Net,&#xA;&#x9;[GLN] = tb.GLN,&#xA;&#x9;[EGRPOU] = tb.EGRPOU,&#xA;&#x9;[INN] = tb.INN,&#xA;&#x9;[SertificateNumber] = tb.SertificateNumber,&#xA;&#x9;IsBuyer = tb.IsBuyer,&#xA;&#x9;IsProvider = tb.IsProvider,&#xA;&#x9;PostAddress = tb.PostAddress,&#xA;&#x9;LegalAddress = tb.LegalAddress,&#xA;&#x9;MarketingChannelCode = tb.MarketingChannelCode,&#xA;&#x9;MarketingChannel = tb.MarketingChannel&#xA;FROM dim.Buyers AS b WITH (NOLOCK) INNER JOIN dbo.tmp_BuyersBKK AS tb&#xA;ON b.BuyerCode = tb.BuyerCode AND b.Source_ID = tb.Source_ID&#xA;WHERE b.Source_ID = 2&#xA;&#xA;-- 2&#xA;UPDATE dim.Buyers&#xA;SET&#xA;    [SK_Main_Buyer_ID] = b.SK_Buyer_ID&#xA;FROM dim.Buyers b&#xA;WHERE b.SK_Main_Buyer_ID = -777&#xA;AND b.Source_ID = 2&#xA;&#xA;-- 3 Обновление Калана сбыта МКИ и ПДХ&#xA;UPDATE dim.Buyers&#xA;SET&#xA;    SalesChannelMkiPdh_ID = scmp.SK_SalesChannelMkiPdh_ID&#xA;FROM dim.Buyers AS b WITH (NOLOCK)&#xA;    INNER JOIN&#xA;    dbo.tmp_BuyersBKK AS tb WITH (NOLOCK) &#xA;&#x9;   ON &#xA;&#x9;   b.BuyerCode = tb.BuyerCode&#xA;&#x9;&#x9;  AND&#xA;&#x9;   b.Source_ID = tb.Source_ID&#xA;    INNER JOIN&#xA;    dim.SalesChannelMkiPdh AS scmp WITH (NOLOCK)&#xA;&#x9;   ON&#xA;&#x9;&#x9; tb.SalesChanelMKV_PDH = scmp.SalesChannelMkiPdhName&#xA;WHERE&#xA;    b.SalesChannelMkiPdh_ID &lt;&gt; scmp.SK_SalesChannelMkiPdh_ID&#xA;&#xA;&#xA;-- 4 Обновление принадлежности Контрагента к Сети&#xA;UPDATE dim.Buyers&#xA;SET&#xA;    Chain_ID = c.Chain_ID&#xA;FROM dim.Buyers AS b WITH (NOLOCK)&#xA;    INNER JOIN dim.Chains AS c WITH (NOLOCK)&#xA;    ON b.Net = c.ChainName&#xA;WHERE b.Source_ID = 2" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>

                  SQLTask:Connection="{31F49E8F-113E-4CDE-B8E9-EEB47A9243CE}"
                  SQLTask:SqlStatementSource="-- Fill date to the fields: DocNumber. DocDate, SK_DocDate_ID&#xA;UPDATE fact.ReturnsMax&#xA;&#x9;SET&#xA;&#x9;&#x9;DocNumber =&#x9;CASE &#xA;&#x9;&#x9;&#x9;WHEN left(r.[Document], 4) = 'Това' THEN left(right(r.[Document], LEN(r.[Document]) - 32), 11)&#xA;&#x9;&#x9;&#x9;WHEN left(r.[Document], 4) = 'Возв' THEN left(right(r.[Document], LEN(r.[Document]) - 41), 11)&#xA;&#x9;&#x9;&#x9;ELSE CAST(left(r.[Document], 11) AS char(11))&#xA;&#x9;&#x9;&#x9;END, &#xA;&#x9;&#x9;DocDate = DATETIMEFROMPARTS(&#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 13), 4) AS INT), -- Год&#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 16), 2) AS INT), -- Месяц&#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 19), 2) AS INT), -- День&#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 8), 2) AS INT), -- Часов&#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 5), 2) AS INT), -- Минут&#xA;&#x9;&#x9;&#x9;CAST(right([Document], 2) AS INT), -- Секунд&#xA;&#x9;&#x9;&#x9;0 -- Милисекунд&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;),&#xA;&#x9;&#x9;SK_DocDate_ID = &#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 13), 4) AS INT) * 10000 + &#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 16), 2) AS INT) * 100 + &#xA;&#x9;&#x9;&#x9;CAST(left(right([Document], 19), 2) AS INT)&#xA;FROM fact.ReturnsMax AS r WITH (NOLOCK)&#xA;WHERE r.Source_ID = 2&#xA;&#x9;AND r.DocDate IS NULL&#xA;&#xA;&#xA;-- Fill data to the field SK_ShipDate_ID&#xA;UPDATE fact.ReturnsMax&#xA;&#x9;SET SK_ShipDate_ID = CASE&#xA;            WHEN rm.SK_Date_ID &gt; rm.SK_DocDate_ID&#xA;            THEN CAKE_WH.dbo.ufn_DateToInt(DATEADD(DAY, 1, rm.DocDate))&#xA;            ELSE rm.SK_DocDate_ID&#xA;    END&#xA;FROM fact.ReturnsMax AS rm WITH (NOLOCK)&#xA;WHERE rm.SK_ShipDate_ID IS NULL&#xA;&#x9;AND rm.Source_ID = 2" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

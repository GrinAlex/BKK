          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{31F49E8F-113E-4CDE-B8E9-EEB47A9243CE}"
              SQLTask:SqlStatementSource="-- обновление Группировки и кода группировки&#xA;UPDATE&#xA;dim.Products&#xA;SET&#xA;GrouppingName = p2.GrouppingName,&#xA;Groupping_ID = p2.Groupping_ID&#xA;&#xA;FROM dim.Products AS P WITH (NOLOCK) INNER JOIN (&#xA;&#xA;select distinct p.GroupName, p.GrouppingName, p.Groupping_ID&#xA;from dim.Products p WITH (NOLOCK)&#xA;where p.GrouppingName IS NOT NULL&#xA;) AS p2 ON p.GroupName = p2.GroupName&#xA;&#xA;&#xA;-- Добавление значение веса для пересчета по пустым записям&#xA;UPDATE&#xA;dim.Products&#xA;SET &#xA;WeightForRecalculating = p.[Weight]&#xA;FROM dim.Products AS p WITH (NOLOCK)&#xA;WHERE p.WeightForRecalculating IS NULL&#xA;&#xA;&#xA;-- Обновление полей в справочнике&#xA;UPDATE &#xA;dim.Products&#xA;SET&#xA;&#x9;ProductName = pt.ProductName,&#xA;&#x9;ProductFullName = pt.ProductFullName&#xA;FROM dim.Products AS p WITH (NOLOCK)&#xA;INNER JOIN dbo.tmp_ProductsBKK AS pt WITH (NOLOCK) ON p.ProductCode = pt.ProductCode&#xA;WHERE p.Sourse_ID = 2&#xA;&#xA;-- Обновление групп продукции не проводить или проводить вручную иначе полетят группы и группировки по лавашам и друой продукции" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable
          DTS:refId="Package\Обновление справочников и таблицы Shipments\Добавление информации в dim Outlets"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:Disabled="True"
          DTS:DTSID="{4F657E97-1660-49F5-B326-ECB3E877BD8D}"

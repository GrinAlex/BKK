          DTS:ObjectName="Добавление информации в dim Outlets"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2016; © 2015 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{31F49E8F-113E-4CDE-B8E9-EEB47A9243CE}"
              SQLTask:SqlStatementSource="  --Update Empry fields&#xA;UPDATE dim.Outlets&#xA;  SET&#xA;      BuyerGroupCode = tob.BuyerGroupCode,&#xA;&#x9;  [BuyerGroupName] = tob.BuyerGroupName,&#xA;      [LogisticCode] = tob.LogisticCode,&#xA;      [TransportCode] = tob.TransportCode,&#xA;      [DeliveryAddress] = tob.DeliveryAddress,&#xA;      [Territory_ID] = da.Territory_ID,&#xA;      [DeliveryArea_ID] = da.DeliveryArea_ID,&#xA;      [Locality] = ISNULL(tob.Locality, '')&#xA;FROM dim.Outlets o WITH (NOLOCK)&#xA;     INNER JOIN dbo.tmp_OutletsBKK tob WITH (NOLOCK) ON o.BuyerCode = tob.BuyerCode&#xA;                                                        AND o.AddressCode = tob.AddressCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; AND o.Source_ID = tob.Source_ID&#xA;     INNER JOIN dim.DeliveryArea da WITH (NOLOCK) ON tob.DeliveryRegion = da.DeliveryAreaName&#xA;WHERE  --o.DeliveryAddress = 'Новый адрес доставки'&#xA;&#x9;--&#x9;AND &#xA;o.Source_ID = 2;&#xA;&#xA;&#xA;    -- Update New Names DeliveryAdress, NotActual TT, Locality&#xA;UPDATE dim.Outlets&#xA;  SET&#xA;      [DeliveryAddress] = tob.DeliveryAddress,&#xA;&#x9; NotActual = tob.NotActual,&#xA;&#x9; Locality = ISNULL(tob.Locality, '')&#xA;FROM dim.Outlets o WITH (NOLOCK)&#xA;     INNER JOIN dbo.tmp_OutletsBKK tob WITH (NOLOCK) ON o.BuyerCode = tob.BuyerCode&#xA;                                                        AND o.AddressCode = tob.AddressCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; AND o.Source_ID = tob.Source_ID&#xA; WHERE o.Source_ID = 2;&#xA;&#xA;&#xA;&#xA;-- Update TransportCode 0 to NULL&#xA;UPDATE dim.Outlets&#xA;  SET&#xA;      TransportCode = NULL&#xA;FROM dim.Outlets o WITH (NOLOCK)&#xA;WHERE o.TransportCode = 0&#xA;      AND o.Source_ID = 2;&#xA;&#xA;--Update Адрес не определен&#xA;UPDATE dim.Outlets&#xA;  SET&#xA;      DeliveryAddress = 'Адрес не определен'&#xA;FROM dim.Outlets o WITH (NOLOCK)&#xA;WHERE o.DeliveryAddress = 'Новый адрес доставки'&#xA;      AND o.TransportCode IS NULL&#xA;&#x9; AND o.Source_ID = 2;&#xA;&#xA;-- Update SK_Buyer_ID, BuyerGroupName, LogisticCode&#xA;UPDATE dim.Outlets&#xA;  SET&#xA;      [SK_Buyer_ID] = b.SK_Buyer_ID,&#xA;&#x9; BuyerGroupName = b.BuyerGroupName,&#xA;&#x9; LogisticCode = b.LogisticCode&#xA;FROM dim.Outlets o WITH (NOLOCK) INNER JOIN dim.Buyers b WITH (NOLOCK) ON o.BuyerCode = b.BuyerCode AND o.Source_ID = b.Source_ID&#xA;WHERE (o.SK_Buyer_ID IS NULL&#xA;or o.BuyerGroupName IS NULL&#xA;or o.LogisticCode IS NULL)&#xA;AND o.Source_ID = 2;&#xA;&#xA;  --Update SK_Main_Outlet_ID&#xA;UPDATE dim.Outlets&#xA;  SET&#xA;      dim.Outlets.SK_Main_Outlet_ID = o.SK_Outlet_ID&#xA;FROM dim.Outlets o&#xA;WHERE (o.SK_Main_Outlet_ID IS NULL&#xA;      OR o.SK_Main_Outlet_ID = -777)&#xA;&#x9; AND o.Source_ID = 2&#xA;&#x9; ;  &#xA;&#xA; -- Update SK_Outlet_ID_Old&#xA;UPDATE dim.Outlets&#xA;  SET&#xA;      dim.Outlets.SK_Outlet_ID_Old = dbo.ufn_LkTkToSk(o.LogisticCode, ISNULL(o.TransportCode,0))&#xA;FROM dim.Outlets o&#xA;WHERE (o.SK_Outlet_ID_Old IS NULL&#xA;      OR o.SK_Outlet_ID_Old = -777&#xA;&#x9; OR o.SK_Outlet_ID_Old = 0)&#xA;&#x9; AND o.Source_ID = 2;&#xA;&#xA;-- Update Field IsRelatedToTOP100&#xA;--1. Обнуление&#xA;UPDATE dim.Outlets&#xA;SET IsRelatedToTOP100 = 0&#xA;FROM dim.Outlets&#xA;&#xA;&#xA;--2. Обновление&#xA;UPDATE dim.Outlets&#xA;SET IsRelatedToTOP100 = 1&#xA;FROM dim.Outlets&#xA;WHERE SK_Outlet_ID IN (SELECT SK_Outlet_ID FROM fact.Plan_By_TopTT WHERE Valid = 1) " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>

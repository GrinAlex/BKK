DECLARE @SalesChannelMkiPdh binary(16) = 0x80D2005056A1748611E6765AE65202EE		-- Свойство "Канал збуту МКВ і ПДХ" объеката
DECLARE @Net binary(16) = 0x80D2005056A1748611E66903F2ED3F47					-- Свойство "Сеть" объеката
DECLARE @PostAddress binary(16) = 0xBC597D7DEEC26CDE42DE22114E115265			-- Свойство Фактический (почтовый) адрес
DECLARE @LegalAddress binary(16) = 0xB0DFBC705ACB83ED44E89CC8B910ECEF			-- Свойство Юридический адрес 
DECLARE @IsPrepaidWorkOnly binary(16) = 0x80D9484D7EE7B5D211EA74D48EE56253		-- Свойство "Работа только по предоплате"

select 
	[Справочник.Контрагенты]._IDRRef as IDRRef							-- Ссылка
	,CAST([Справочник.Контрагенты]._Marked AS bit) as IsDeleted			-- ПометкаУдаления
	,[Справочник.Контрагенты]._ParentIDRRef as ParentIDRRef				-- Родитель_Ссылка
	,CASE 
		WHEN CAST([Справочник.Контрагенты]._Folder as bit) = 0 THEN 1
		ELSE 0
	END as IsGroup														-- ЭтоГруппа
	,[Справочник.Контрагенты]._Code as BuyerCode						-- Код
	,[Справочник.Контрагенты]._Description as BuyerName					-- Наименование
	,[Справочник.Контрагенты]._Fld1924 as BuyerFullName					-- НаименованиеПолное
	,CAST([Справочник.Контрагенты]._Fld1936 AS bit) as IsBuyer			-- Покупатель
	,CAST([Справочник.Контрагенты]._Fld1937 as bit) AS IsProvider		-- Поставщик
	,[Справочник.Контрагенты]._Fld1930 as INN							-- ИНН
	,[Справочник.Контрагенты]._Fld1941 as EGRPOU						-- КодПоЕДРПОУ
	,[Справочник.Контрагенты]._Fld1942 as SertificateNumber				-- НомерСвидетельства
	,[Справочник.Контрагенты]._Fld1945 as LogisticCode					-- ЛогистическийКод
	,[Справочник.Контрагенты]._Fld23171 as GLN							-- КодGLN
	,sc._IDRRef AS SalesChannelMkiPdhIDRRef
	,sc._Code AS [SalesChannelMkiPdhCode]
	,sc._Description AS [SalesChannelMkiPdhName]
	,n._IDRRef AS ChainIDRRef
	,n._Code AS NetCode
	,n._Description AS [Net]
	,pa.PostAddress
	,CASE
		WHEN [Справочник.Контрагенты]._Fld1940RRef = 0xB3917A6DC08EA0F64F59C4DEEEF13A4C THEN N'Юр. лицо'
		WHEN [Справочник.Контрагенты]._Fld1940RRef = 0xA13EB9B8FC82A2864720BAFAD2CFF47A THEN N'Физ. лицо'
	END AS	LegalPerson
	,la.LegalAddress
	,CAST(prepaid._Fld18165_L AS bit) AS IsPrepaidWorkOnly
  FROM [DB_1C].[UPP_BKK].[dbo].[_Reference116] AS [Справочник.Контрагенты] WITH (NOLOCK)
  LEFT OUTER JOIN 
  -- Канал сбыта МКИ и ПДХ
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld18163_RRRef
		,[Справочник.ЗначенияСвойствОбъектов]._IDRRef
		,[Справочник.ЗначенияСвойствОбъектов]._Description
		,[Справочник.ЗначенияСвойствОбъектов]._Code
  FROM [DB_1C].[UPP_BKK].[dbo].[_InfoRg18162] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
	INNER JOIN [DB_1C].[UPP_BKK].[dbo].[_Reference90] AS [Справочник.ЗначенияСвойствОбъектов] WITH (NOLOCK) 
		ON [РС.ЗначенияСвойствОбъектов].[_Fld18165_RRRef] = [Справочник.ЗначенияСвойствОбъектов]._IDRRef
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld18164RRef] = @SalesChannelMkiPdh) AS sc
   ON [Справочник.Контрагенты]._IDRRef = sc._Fld18163_RRRef
LEFT OUTER JOIN
-- Сеть
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld18163_RRRef
		,[Справочник.ЗначенияСвойствОбъектов]._IDRRef
		,[Справочник.ЗначенияСвойствОбъектов]._Description
		,[Справочник.ЗначенияСвойствОбъектов]._Code
  FROM [DB_1C].[UPP_BKK].[dbo].[_InfoRg18162] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
	INNER JOIN [DB_1C].[UPP_BKK].[dbo].[_Reference90] AS [Справочник.ЗначенияСвойствОбъектов] WITH (NOLOCK) 
		ON [РС.ЗначенияСвойствОбъектов].[_Fld18165_RRRef] = [Справочник.ЗначенияСвойствОбъектов]._IDRRef
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld18164RRef] = @Net) AS n
   ON [Справочник.Контрагенты]._IDRRef = n._Fld18163_RRRef
LEFT OUTER JOIN
	-- Фактический (почтовый) адрес
	(SELECT
		rs._Fld18326_RRRef
		,rs._Fld18329 AS PostAddress
	FROM [DB_1C].[UPP_BKK].[dbo].[_InfoRg18325] AS rs WITH (NOLOCK)
	WHERE rs.[_Fld18328_RRRef] = @PostAddress
	) AS pa
	ON [Справочник.Контрагенты]._IDRRef = pa._Fld18326_RRRef
LEFT OUTER JOIN
	-- Юридический адрес
	(SELECT
		rs._Fld18326_RRRef
		,rs._Fld18329 AS LegalAddress
	FROM [DB_1C].[UPP_BKK].[dbo].[_InfoRg18325] AS rs WITH (NOLOCK)
	WHERE rs.[_Fld18328_RRRef] = @LegalAddress
	) AS la
	ON [Справочник.Контрагенты]._IDRRef = la._Fld18326_RRRef
LEFT OUTER JOIN
	-- Работа только по предоплате
  (SELECT
		[РС.ЗначенияСвойствОбъектов]._Fld18163_RRRef
		,[РС.ЗначенияСвойствОбъектов]._Fld18165_L
  FROM [DB_1C].[UPP_BKK].[dbo].[_InfoRg18162] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld18164RRef] = @IsPrepaidWorkOnly) AS prepaid
   ON [Справочник.Контрагенты]._IDRRef = prepaid._Fld18163_RRRef</property>

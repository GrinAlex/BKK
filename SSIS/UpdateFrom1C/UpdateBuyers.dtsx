                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--------- Стол заказов ----------------
DECLARE @SalesChannelMkiPdh binary(16) = 0x80D2005056A1748611E6765AE65202EE		-- Свойство "Канал збуту МКВ і ПДХ" объеката
DECLARE @Net binary(16) = 0x80D2005056A1748611E66903F2ED3F47					-- Свойство "Сеть" объеката
DECLARE @PostAddress binary(16) = 0xBC597D7DEEC26CDE42DE22114E115265			-- Свойство Фактический (почтовый) адрес
DECLARE @LegalAddress binary(16) = 0xB0DFBC705ACB83ED44E89CC8B910ECEF			-- Свойство Юридический адрес 
DECLARE @IsPrepaidWorkOnly binary(16) = 0x80D9484D7EE7B5D211EA74D48EE56253		-- Свойство "Работа только по предоплате"

select
	[Справочник.Контрагенты]._IDRRef as IDRRef							-- Ссылка
	,CAST([Справочник.Контрагенты]._Marked AS bit) as IsDeleted			-- ПометкаУдаления
	,[Справочник.Контрагенты]._ParentIDRRef as ParentIDRRef				-- Родитель_Ссылка
	,CASE
		WHEN CAST([Справочник.Контрагенты]._Folder as bit) = 0 THEN 1
		ELSE 0
	END as IsGroup														-- ЭтоГруппа
	,[Справочник.Контрагенты]._Code as BuyerCode						-- Код
	,[Справочник.Контрагенты]._Description as BuyerName					-- Наименование
	,[Справочник.Контрагенты]._Fld746 as BuyerFullName					-- НаименованиеПолное
	,CAST([Справочник.Контрагенты]._Fld749 AS bit) as IsBuyer			-- Покупатель
	,CAST([Справочник.Контрагенты]._Fld750 as bit) AS IsProvider		-- Поставщик
	,[Справочник.Контрагенты]._Fld748 as INN							-- ИНН
	,[Справочник.Контрагенты]._Fld753 as EGRPOU							-- КодПоЕДРПОУ
	,[Справочник.Контрагенты]._Fld754 as SertificateNumber				-- НомерСвидетельства
	,[Справочник.Контрагенты]._Fld762 as LogisticCode					-- ЛогистическийКод
	,[Справочник.Контрагенты]._Fld767 as GLN							-- КодGLN
	,sc._IDRRef AS SalesChannelMkiPdhIDRRef
	,sc._Code AS [SalesChannelMkiPdhCode]
	,sc._Description AS [SalesChannelMkiPdhName]
	,n._IDRRef AS ChainIDRRef
	,n._Code AS NetCode
	,n._Description AS [Net]
	,[Справочник.Контрагенты]._Fld765 AS PostAddress
	,CASE
		WHEN [Справочник.Контрагенты]._Fld752RRef = 0xA017DC8143907B4D46E05F008C7413CB THEN N'Юр. лицо'
		WHEN [Справочник.Контрагенты]._Fld752RRef = 0xA10A0E4C6592904547261C723A897FEA THEN N'Физ. лицо'
	END AS	LegalPerson
	,[Справочник.Контрагенты]._Fld763 AS LegalAddress 
	,CAST(prepaid._Fld1842_L AS bit) AS IsPrepaidWorkOnly
  FROM [DB_1C].[ORD_BKK].[dbo].[_Reference41] AS [Справочник.Контрагенты] WITH (NOLOCK)
  LEFT OUTER JOIN 
  -- Канал сбыта МКИ и ПДХ
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld1840_RRRef
		,[Справочник.ЗначенияСвойствОбъектов]._IDRRef
		,[Справочник.ЗначенияСвойствОбъектов]._Description
		,[Справочник.ЗначенияСвойствОбъектов]._Code
  FROM [DB_1C].[ORD_BKK].[dbo].[_InfoRg1839] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
	INNER JOIN [DB_1C].[ORD_BKK].[dbo].[_Reference24] AS [Справочник.ЗначенияСвойствОбъектов] WITH (NOLOCK) 
		ON [РС.ЗначенияСвойствОбъектов].[_Fld1842_RRRef] = [Справочник.ЗначенияСвойствОбъектов]._IDRRef
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld1841RRef] = @SalesChannelMkiPdh) AS sc
   ON [Справочник.Контрагенты]._IDRRef = sc._Fld1840_RRRef
LEFT OUTER JOIN
-- Сеть
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld1840_RRRef
		,[Справочник.ЗначенияСвойствОбъектов]._IDRRef
		,[Справочник.ЗначенияСвойствОбъектов]._Description
		,[Справочник.ЗначенияСвойствОбъектов]._Code
  FROM [DB_1C].[ORD_BKK].[dbo].[_InfoRg1839] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
	INNER JOIN [DB_1C].[ORD_BKK].[dbo].[_Reference24] AS [Справочник.ЗначенияСвойствОбъектов] WITH (NOLOCK) 
		ON [РС.ЗначенияСвойствОбъектов].[_Fld1842_RRRef] = [Справочник.ЗначенияСвойствОбъектов]._IDRRef
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld1841RRef] = @Net) AS n
   ON [Справочник.Контрагенты]._IDRRef = n._Fld1840_RRRef
LEFT OUTER JOIN
-- Работа только по предоплате
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld1840_RRRef
		,[РС.ЗначенияСвойствОбъектов]._Fld1842_L
  FROM [DB_1C].[ORD_BKK].[dbo].[_InfoRg1839] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld1841RRef] = @IsPrepaidWorkOnly) AS prepaid
   ON [Справочник.Контрагенты]._IDRRef = prepaid._Fld1840_RRRef
LEFT JOIN CAKE_ST.dim.Buyers AS b WITH (NOLOCK) 
		ON [Справочник.Контрагенты]._IDRRef = b.IDRRef AND b.Source_ID = 2

  WHERE b.IDRRef IS NULL</property>

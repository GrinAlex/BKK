                    <property
                      dataType="System.String"
                      description="Команда SQL для выполнения."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--------- Сбыт ----------------
DECLARE @SalesChannelMkiPdh binary(16) = 0x80D2005056A1748611E6765AE65202EE		-- Свойство "Канал збуту МКВ і ПДХ" объеката
DECLARE @Net binary(16) = 0x80D2005056A1748611E66903F2ED3F47					-- Свойство "Сеть" объеката
DECLARE @PostAddress binary(16) = 0xBC597D7DEEC26CDE42DE22114E115265			-- Свойство Фактический (почтовый) адрес
DECLARE @LegalAddress binary(16) = 0xB0DFBC705ACB83ED44E89CC8B910ECEF			-- Свойство Юридический адрес 
DECLARE @IsPrepaidWorkOnly binary(16) = 0x80D9484D7EE7B5D211EA74D48EE56253		-- Свойство "Работа только по предоплате"

select
	[Справочник.Контрагенты]._IDRRef as IDRRef							-- Ссылка
	,CAST([Справочник.Контрагенты]._Marked AS bit) as IsDeleted			-- ПометкаУдаления
	,[Справочник.Контрагенты]._ParentIDRRef as ParentIDRRef				-- Родитель_Ссылка
	,CASE
		WHEN CAST([Справочник.Контрагенты]._Folder as bit) = 0 THEN 1
		ELSE 0
	END as IsGroup			-- ЭтоГруппа
	,[Справочник.Контрагенты]._Code as BuyerCode						-- Код
	,[Справочник.Контрагенты]._Description as BuyerName					-- Наименование
	,[Справочник.Контрагенты]._Fld262 as BuyerFullName					-- НаименованиеПолное
	,CAST([Справочник.Контрагенты]._Fld265 AS bit) as IsBuyer			-- Покупатель
	,CAST([Справочник.Контрагенты]._Fld266 as bit) AS IsProvider		-- Поставщик
	,[Справочник.Контрагенты]._Fld264 as INN							-- ИНН
	,[Справочник.Контрагенты]._Fld269 as EGRPOU							-- КодПоЕДРПОУ
	,[Справочник.Контрагенты]._Fld270 as SertificateNumber				-- НомерСвидетельства
	,[Справочник.Контрагенты]._Fld278 as LogisticCode					-- ЛогистическийКод
	,[Справочник.Контрагенты]._Fld1689 as GLN							-- КодGLN
	,sc._IDRRef AS SalesChannelMkiPdhIDRRef
	,sc._Code AS [SalesChannelMkiPdhCode]
	,sc._Description AS [SalesChannelMkiPdhName]
	,n._IDRRef AS ChainIDRRef
	,n._Code AS NetCode
	,n._Description AS [Net]
	,[Справочник.Контрагенты]._Fld281 AS PostAddress
	,CASE
		WHEN [Справочник.Контрагенты]._Fld268RRef = 0xA017DC8143907B4D46E05F008C7413CB THEN N'Юр. лицо'
		WHEN [Справочник.Контрагенты]._Fld268RRef = 0xA10A0E4C6592904547261C723A897FEA THEN N'Физ. лицо'
	END AS	LegalPerson
	,[Справочник.Контрагенты]._Fld279 AS LegalAddress 
	,CAST(prepaid._Fld1092_L AS bit) AS IsPrepaidWorkOnly
  FROM [DB_1C].[EXP_BKK].[dbo].[_Reference26] AS [Справочник.Контрагенты] WITH (NOLOCK)
  LEFT OUTER JOIN 
  -- Канал сбыта МКИ и ПДХ
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld1090_RRRef
		,[Справочник.ЗначенияСвойствОбъектов]._IDRRef
		,[Справочник.ЗначенияСвойствОбъектов]._Description
		,[Справочник.ЗначенияСвойствОбъектов]._Code
  FROM [DB_1C].[EXP_BKK].[dbo].[_InfoRg1089] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
	INNER JOIN [DB_1C].[EXP_BKK].[dbo].[_Reference22] AS [Справочник.ЗначенияСвойствОбъектов] WITH (NOLOCK) 
		ON [РС.ЗначенияСвойствОбъектов].[_Fld1092_RRRef] = [Справочник.ЗначенияСвойствОбъектов]._IDRRef
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld1091RRef] = @SalesChannelMkiPdh) AS sc
   ON [Справочник.Контрагенты]._IDRRef = sc._Fld1090_RRRef
LEFT OUTER JOIN
-- Сеть
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld1090_RRRef
		,[Справочник.ЗначенияСвойствОбъектов]._IDRRef
		,[Справочник.ЗначенияСвойствОбъектов]._Description
		,[Справочник.ЗначенияСвойствОбъектов]._Code
  FROM [DB_1C].[EXP_BKK].[dbo].[_InfoRg1089] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
	INNER JOIN [DB_1C].[EXP_BKK].[dbo].[_Reference22] AS [Справочник.ЗначенияСвойствОбъектов] WITH (NOLOCK) 
		ON [РС.ЗначенияСвойствОбъектов].[_Fld1092_RRRef] = [Справочник.ЗначенияСвойствОбъектов]._IDRRef
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld1091RRef] = @Net) AS n
   ON [Справочник.Контрагенты]._IDRRef = n._Fld1090_RRRef
LEFT OUTER JOIN
-- Работа только по предоплате
  (SELECT 
		[РС.ЗначенияСвойствОбъектов]._Fld1090_RRRef
		,[РС.ЗначенияСвойствОбъектов]._Fld1092_L
  FROM [DB_1C].[EXP_BKK].[dbo].[_InfoRg1089] AS [РС.ЗначенияСвойствОбъектов] WITH (NOLOCK)
  WHERE [РС.ЗначенияСвойствОбъектов].[_Fld1091RRef] = @IsPrepaidWorkOnly) AS prepaid
   ON [Справочник.Контрагенты]._IDRRef = prepaid._Fld1090_RRRef
LEFT JOIN CAKE_ST.dim.Buyers AS b WITH (NOLOCK) 
		ON [Справочник.Контрагенты]._IDRRef = b.IDRRef AND b.Source_ID = 2
WHERE b.IDRRef IS NULL</property>

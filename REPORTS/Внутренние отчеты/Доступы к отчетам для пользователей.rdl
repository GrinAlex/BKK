<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="OLTP_CAKE_WH">
      <DataSourceReference>OLTP_CAKE_WH</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>d4fe4210-38fc-4341-9f6b-4e807588985a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="AccessToReports">
      <Query>
        <DataSourceName>OLTP_CAKE_WH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@paramLogin">
            <Value>=Parameters!paramLogin.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	t.Type
	,t.Path
	,t.Report
	,t.UserName
	,t.Login
	,t.RoleName
	,ISNULL(p.FullName, t.UserName) AS FullName
	,ISNULL(p.ShortName, ' - ') AS ShortName
	,ISNULL(p.Mail, ' - ') AS Mail
	,ISNULL(p.Phone, ' - ') AS Phone
FROM
(SELECT  CASE RSCatalog.TYPE
          WHEN 1 THEN 'Folder'
          ELSE 'Report'
        END AS [Type] ,
        RSCatalog.Path ,
        RSCatalog.Name AS Report ,
        Users.UserName ,
		RIGHT(Users.UserName, LEN(Users.UserName) - CHARINDEX('\', Users.UserName)) AS [Login] ,
        Roles.RoleName
FROM    ReportServer.[dbo].[Catalog] RSCatalog
        INNER JOIN ReportServer.[dbo].[PolicyUserRole] PolicyUserRole WITH (NOLOCK) ON RSCatalog.PolicyID = PolicyUserRole.PolicyID
        INNER JOIN ReportServer.[dbo].[Roles] Roles WITH (NOLOCK) ON PolicyUserRole.RoleID = Roles.RoleID
        INNER JOIN ReportServer.[dbo].[Users] Users WITH (NOLOCK) ON PolicyUserRole.UserID = Users.UserID
WHERE   RSCatalog.TYPE IN ( 1, 2 )
AND RIGHT(Users.UserName, LEN(Users.UserName) - CHARINDEX('\', Users.UserName)) = @paramLogin
) AS t
	LEFT OUTER JOIN CAKE_WH.adm.Peoples AS p WITH (NOLOCK) ON t.Login = p.Login COLLATE Ukrainian_CI_AS</CommandText>
      </Query>
